{
  "PFD old template": {
    "prefix": "pfd_old",
    "body": [
      "```puml",
      "!include pfd_tool.pu",
      "!\\$space_delimiter = 1",
      "left to right direction",
      "skinparam linetype polyline",
      "skinparam linetype ortho",
      "",
      "\\$d( \\",
      "    \"$1out\", \\",
      "    \"$2in1,in2 c:in3\", \\",
      "    \"$3process\" \\",
      ")",
      "\\$c(\"#pink\", \"in1\")",
      "\\$cp(\"#skyblue\", \"process\")",
      "```"
    ],
    "description": "PFD old template"
  },
  "draft process": {
    "prefix": "d",
    "body": [
      "\\$d( \\",
      "    \"$1out\", \\",
      "    \"$2in\", \\",
      "    \"$3process\" \\",
      ")"
    ],
    "description": "draft process"
  },
  "draft process from clipboard": {
    "prefix": "c",
    "body": [
      "\\$d( \\",
      "    \"$1$CLIPBOARD\", \\",
      "    \"$2$CLIPBOARD\", \\",
      "    \"$3Process${CURRENT_MINUTE}${CURRENT_SECOND}\" \\",
      ")"
    ],
    "description": "draft process from clipboard"
  },
  "replace procedure from clipboard": {
    "prefix": "r",
    "body": [
      "\\$replace(\"$CLIPBOARD\")"
    ],
    "description": "replace procedure from clipboard"
  },
  "PFD template": {
    "prefix": "pfd",
    "body": [
      "```puml",
      "!include pfd_tool.pu",
      "!\\$numbered = 1",
      "!\\$target_length = 15",
      "!\\$newline_at_space = 0",
      "left to right direction",
      "skinparam linetype polyline",
      "skinparam linetype ortho",
      "",
      "\\$entities(\"\\",
      "out,\\",
      "in1#pink,\\",
      "in2,\\",
      "c:in3\\",
      "\")",
      "",
      "\\$p(\"2 3 4 > 1\", \"process#skyblue\")",
      "```"
    ],
    "description": "PFD template"
  },
  "process": {
    "prefix": "p",
    "body": [
      "\\$p(\"2 3 4 > 1\", \"process#skyblue\")"
    ],
    "description": "PFD process"
  },
  "multi line note": {
    "prefix": "n",
    "body": [
      "note as n$CURRENT_MINUTE$CURRENT_SECOND",
      "$1",
      "end note",
      "n$CURRENT_MINUTE$CURRENT_SECOND .. YOUR_ALIAS",
      ""
    ],
    "description": "multi line note"
  },
  "multi line note to clipboard": {
    "prefix": "nc",
    "body": [
      "note as n$CURRENT_MINUTE$CURRENT_SECOND",
      "$1",
      "end note",
      "n$CURRENT_MINUTE$CURRENT_SECOND .. \\$replace(\"$CLIPBOARD\")",
      ""
    ],
    "description": "multi line note to clipboard"
  },
  "define color for deliverables": {
    "prefix": "c",
    "body": [
      "\\$c(\"#pink\", \"in1\")"
    ],
    "description": "define color for deliverables"
  },
  "define color for process": {
    "prefix": "cp",
    "body": [
      "\\$cp(\"#pink\", \"process\")"
    ],
    "description": "define color for process"
  },
  "CRT template": {
    "prefix": "crt",
    "body": [
      "```puml",
      "!include crt_tool.pu",
      "!\\$numbered = 1",
      "!\\$target_length = 15",
      "!\\$newline_at_space = 0",
      "skinparam linetype polyline",
      "skinparam linetype ortho",
      "",
      "\\$entities(\"\\",
      "$1UDE #pink,\\",
      "$2cause1,\\",
      "$3cause2,\\",
      "$3cause3,\\",
      "\")",
      "",
      "\\$connect_result_from_causes(\"\\",
      "2 3 4 > 1,\\",
      "\")",
      "```"
    ],
    "description": "CRT template"
  },
  "REQ template": {
    "prefix": "req",
    "body": [
      "```puml",
      "!include req_tool.pu",
      "!\\$numbered = 1",
      "!\\$target_length = 15",
      "!\\$newline_at_space = 1",
      "!\\$detailed = 1",
      "skinparam linetype polyline",
      "skinparam linetype ortho",
      "",
      "\\$req(\"\\",
      "$1RequirementA#pink,Detailed description...,\\",
      "$2RequirementB, ,\\",
      "$3RequirementC, ,\\",
      "$3RequirementD, ,\\",
      "\")",
      "",
      "\\$block(\"\\",
      "$1ImplementAlpha,\\",
      "\")",
      "",
      "\\$connect_req(\"\\",
      "2 3 + 1,\\",
      "4 d 2,\\",
      "5 s 3,\\",
      "\")",
      "```"
    ],
    "description": "REQ template"
  },
  "evaporating_cloud": {
    "prefix": "cloud",
    "body": [
      "```puml",
      "!include ec_tool.pu",
      "!\\$space_delimiter = 1",
      "left to right direction",
      "",
      "\\$head(\"$1The mission\")",
      "\\$left( \\",
      "    \"$2Benefits of A\", \\",
      "    \"$3Option A\" \\",
      ")",
      "\\$right( \\",
      "    \"$4Benefits of B\", \\",
      "    \"$5Option B\" \\",
      ")",
      "",
      "' Pros&Cons of each options",
      "note as left_note",
      "pros",
      "*",
      "cons",
      "*",
      "end note",
      "",
      "note as right_note",
      "pros",
      "*",
      "cons",
      "*",
      "end note",
      "",
      "' Assumptions of Option -> Benefits",
      "note as left_ob",
      "end note",
      "",
      "note as right_ob",
      "end note",
      "",
      "' Assumptions of Benefits -> Mission",
      "note as left_bm",
      "end note",
      "",
      "note as right_bm",
      "end note",
      "",
      "\\$connect()",
      "```"
    ],
    "description": "Evaporating cloud"
  }
}